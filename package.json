{
    "name": "crumbs",
    "displayName": "Crumbs",
    "description": "Open Packet Traces in Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCustomEditor:crumbs.editor"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "crumbs.editor",
                "displayName": "Packet Editor",
                "selector": [
                    {
                        "filenamePattern": "*.dmp*"
                    },
                    {
                        "filenamePattern": "*.cap*"
                    },
                    {
                        "filenamePattern": "*.pcap*"
                    },
                    {
                        "filenamePattern": "*.ntar*"
                    },
                    {
                        "filenamePattern": "*.5vw*"
                    },
                    {
                        "filenamePattern": "*.syc*"
                    },
                    {
                        "filenamePattern": "*.fdc*"
                    },
                    {
                        "filenamePattern": "*.trc*"
                    },
                    {
                        "filenamePattern": "*.enc*"
                    },
                    {
                        "filenamePattern": "*.caz*"
                    },
                    {
                        "filenamePattern": "*.erf*"
                    },
                    {
                        "filenamePattern": "*.bfr*"
                    },
                    {
                        "filenamePattern": "*.cscpkt*"
                    },
                    {
                        "filenamePattern": "*.tr1*"
                    },
                    {
                        "filenamePattern": "*.rf5*"
                    },
                    {
                        "filenamePattern": "*.wpz*"
                    },
                    {
                        "filenamePattern": "*.apc*"
                    },
                    {
                        "filenamePattern": "*.tpc*"
                    },
                    {
                        "filenamePattern": "*.pkt*"
                    },
                    {
                        "filenamePattern": "*.mplog*"
                    },
                    {
                        "filenamePattern": "*.ncf*"
                    },
                    {
                        "filenamePattern": "*.pklg*"
                    },
                    {
                        "filenamePattern": "*.dcf*"
                    },
                    {
                        "filenamePattern": "*.ipfix*"
                    },
                    {
                        "filenamePattern": "*.pfx*"
                    },
                    {
                        "filenamePattern": "*.aps*"
                    },
                    {
                        "filenamePattern": "*.mpg*"
                    },
                    {
                        "filenamePattern": "*.ts*"
                    },
                    {
                        "filenamePattern": "*.mp2t*"
                    },
                    {
                        "filenamePattern": "*.vwr*"
                    },
                    {
                        "filenamePattern": "*.camins*"
                    },
                    {
                        "filenamePattern": "*.tnef*"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/ndjson": "^2.0.0",
        "@types/node": "^13.11.0",
        "@types/pug": "^2.0.4",
        "@types/uuid": "^8.3.0",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "async-mutex": "^0.2.6",
        "ndjson": "^2.0.0",
        "pug": "^3.0.0",
        "uuid": "^8.3.2"
    }
}
